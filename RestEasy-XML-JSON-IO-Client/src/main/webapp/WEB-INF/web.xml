<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" version="3.1">
  <display-name>RestEasy-XML-JSON-IO-Client</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <servlet-name>RestEasyClientServlet</servlet-name>
    <servlet-class>com.soumen.restclient.RestEasyClientServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RestEasyClientServlet</servlet-name>
    <url-pattern>/RestEasyClientServlet/*</url-pattern>
  </servlet-mapping>
</web-app>